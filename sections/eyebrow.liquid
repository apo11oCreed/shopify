<div class=" section-{{ section.id }}-padding">
  EYEBROW
</div>

{% schema %}
{
  "name": "t:sections.eyebrow.name",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "eyebrow",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    }
  ]
}
{% endschema %}
